{
	"version": 0,
	"properties":
	{
		"order": [ "width", "height" ],
		"width": { "default": "return 1", "transformer": "return Math.abs(Number.parseFloat(value)) || 1" },
		"height": { "default": "return 1", "transformer": "return Math.abs(Number.parseFloat(value)) || 1" }
	},
	"functions":
	{
		"updateVertices":
		{
			"path": "geometric.plane.updateVertices",
			"type": 1
		},
		"buildTriangles":
		{
			"path": "geometric.plane.buildTriangles",
			"type": 2
		}
	},
	"updateVertices": "return updateVertices.apply(undefined, arguments);",
	"buildTriangles": "return buildTriangles.apply(undefined, arguments);"
}